{
  "name": "alexmdodge",
  "version": "0.9.0",
  "description": "My personal site built using a variety of tools. Used as a portfolio and a way to show a variety of project examples",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"http-server dist -p 9090\" \"npm run build:dev -- --watch\" \"npm run build:static\"",
    "build:dev": "NODE_ENV=dev webpack",
    "build:production": "npm run build:static && NODE_ENV=production webpack",
    "build:static": "npm run clean:dist && npm run copy:assets",
    "clean:dist": "rm -rf ./dist && mkdir ./dist",
    "copy:assets": "cp -r ./app/assets ./dist/",
    "test": "mocha -w -R spec --require babel-register ./test/gridGrindTest.js"
  },
  "author": "Alex Dodge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexmdodge/grid-grind.git"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.10.0",
    "image-webpack-loader": "^3.3.1",
    "jquery": "^3.2.1",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.2",
    "pug": "^2.0.0-rc.1",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.3",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "expose-loader": "^0.7.3",
    "phaser": "^2.6.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "script-loader": "^0.7.0"
  }
}
