{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "pug": true,
    "Phaser": true,
    "$": true,
    "PIXI": true,
    "p2": true
  },
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-unused-vars": ["error", { 
      "varsIgnorePattern": "gridGrind"  
    }]
  }
}